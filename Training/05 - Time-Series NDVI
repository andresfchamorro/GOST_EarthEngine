//-----    1. Get locations of interest from two shapefiles (treatment and control sites)

// Ethiopia AOIs
var treatment = ee.FeatureCollection('users/afche18/treatment_unique');
var addType_treatment = function(feature) {
  feature = feature.set({'type':'treatment'}).buffer(1000);
  return feature;
}
var treatment_mapped = treatment.map(addType_treatment);

var control = ee.FeatureCollection('users/afche18/control_unique');
var addType_control = function(feature) {
  feature = feature.set({'type':'control'}).buffer(1000);
  return feature;
}
var control_mapped = control.map(addType_control);
var sites = control_mapped.merge(treatment_mapped).filterBounds(bbox);

// print(treatment);
// print(treatment_mapped);
// print(control);
// print(control_mapped);
print(sites);

Map.centerObject(bbox, 10);
// Map.addLayer(bbox, {color: 'white'}, 'AOI Extent');
Map.addLayer(control_mapped, {color: 'teal'}, 'control sites');
Map.addLayer(treatment_mapped, {color: 'red'}, 'treatment sites');

//-----   4. Load NDVI time-series dataset

// NDVI Datasets
var terra = ee.ImageCollection('MODIS/006/MOD13Q1')
  .select('NDVI')
  .filterDate('2016-01-01','2018-12-31');
var aqua = ee.ImageCollection('MODIS/006/MYD13Q1')
  .select('NDVI')
  .filterDate('2016-01-01','2018-12-31');
var modis = terra.merge(aqua);

// Functions applied to each image (add timetamp, source, and mask by cropland)
var PrepareModis = function(image) {
  
  return image.clip(bbox)
    .multiply(0.0001)
    // .updateMask(crop_mask)
    .set('date', image.date().format("YYYY_MM_dd"));

};

var modis_clipped = modis.map(PrepareModis);

// Find out how many images we have: 138
// print(modis_clipped.size());
var images_list = modis_clipped.toList(modis_clipped.size());

var reducer_pct = ee.Reducer.percentile([50,75,90]);

// Get Zonal Stats 
var ZonalStats = function(feature) {
  
  for (var i = 0; i < 138; i++){
    
    var image = ee.Image(images_list.get(i));
    var date = ee.String(image.get('date'));

    // Just getting percentiles but you can also combine reducers
    var stats = image.reduceRegion({
      reducer: reducer_pct,
      geometry: feature.geometry(),
      scale: 250,
      maxPixels: 10e15
    });
    
    var name_median = ee.String("median").cat("__").cat(date);
    var name_p75 = ee.String("p75").cat("__").cat(date);
    var name_p90 = ee.String("p90").cat("__").cat(date);
    
    feature = feature.set(
      name_median, stats.get("NDVI_p50"),
      name_p75, stats.get("NDVI_p75"),
      name_p90, stats.get("NDVI_p90")
    );
    
  }
  
  return feature;
  
}

var one_site = sites.filterMetadata('Id','equals',51);
// print(one_site);

var mappedReduction = one_site.map(ZonalStats);
// print(mappedReduction);

